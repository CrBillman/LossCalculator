#!/bin/bash

j=1
i=1

for i in 11  12  14  15	16  18  19  20  21  22  23  3  5  7  8  9;
do

cd $i
mkdir min1_cc min2_cc
cp min1 min1_cc/CONFIG
cp min2 min2_cc/CONFIG

cp ../FIELD ./min1_cc
cp ../FIELD ./min2_cc
cp ../CONTROL_coup ./min1_cc/CONTROL
cp ../CONTROL_coup ./min2_cc/CONTROL
cp ../job_cori_cc ./min1_cc
cp ../job_cori_cc ./min2_cc

cd min1_cc
sbatch job_cori_cc
cd ../min2_cc
sbatch job_cori_cc

cd ..

cd ..


done 
