#!/bin/bash

j=1
i=1

for i in 10 15 18 19 21 23 25 26 28 29 33 34 36 38 40 41 43 44 46 47 49 51 54 59 61 63 64 66 67 70 72 78 80 81 83 88 90 92 93 98 101 102 103 105 108 110 115 121 124 129 132 140 147 149 150 152 154 159 160 162 164 168 170 171 172 173 176 180 181 184 188 189 190 195 198 205 215 216 217 218 224 225 227 229 230 233 234 236 237 249 250 257 264 265 268 271 272 273 281
do

cd $i
mkdir min1_cc min2_cc
cp min1 min1_cc/CONFIG
cp min2 min2_cc/CONFIG

cp ../FIELD ./min1_cc
cp ../FIELD ./min2_cc
cp ../CONTROL_nrcoup ./min1_cc/CONTROL
cp ../CONTROL_nrcoup ./min2_cc/CONTROL
cp ../job_ed_cc ./min1_cc
cp ../job_ed_cc ./min2_cc

cd min1_cc
qsub job_ed_cc
cd ../min2_cc
qsub job_ed_cc

cd ..

cd ..


done 